(* param debugOutput = true. *)

free c.
free zero.
free one.
free two.
free three.

free beep.
private free k.

query attacker:k.

cell s:= zero.

let S =
read s as x;
if x = one then
s:= two.

let T =
read s as x;
if x = zero then
s:= one;
read s as x;
if x = three then
out (c, k).

process S | T
