
free c:channel.
free zero:bitstring.
free one:bitstring.
free two:bitstring.
free three:bitstring.

free beep:bitstring.
free k:bitstring[private].

query attacker(k).

cell s:= zero.

let S =
read s as x;
if x = one then
s:= two.

let T =
read s as x;
if x = zero then
s:= one;
read s as x;
if x = three then
out (c, k).

process S | T
