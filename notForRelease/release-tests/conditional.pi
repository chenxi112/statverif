param verboseClauses=explained.

free c.
private free k.

private free beep.

free one.
free two.
free three.


fun enc/2.

reduc dec(enc(m,k),k) = k.

query attacker:beep.

let A = out(c, enc(three,k)).

let B =
  in(c,xm);
  if dec(xm,k) = two then out(c, one)
  else if dec(xm,k) = three then
  out(c, beep).

process A | B
